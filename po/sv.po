#: src/main.sh:882
msgid "  --allow-downgrades    Allow packages to be downgraded"
msgstr "  --allow-downgrades Tillåt att paket nedgraderas"

#: src/main.sh:881
msgid "  --as-deps             Mark built packages as automatically installed"
msgstr "  --as-deps Markera byggda paket som automatiskt installerade"

#: src/main.sh:893
msgid "  --error               Print an error message to stderr"
msgstr "  --error Skriv ut ett felmeddelande till stderr"

#: src/main.sh:894
msgid "  --error2              Print an error2 message to stderr"
msgstr "  --error2 Skriv ut ett fel2-meddelande till stderr"

#: src/main.sh:872
msgid "  --lint                Link the PKGBUILD for conformity requirements"
msgstr "  --lint Länka PKGBUILD för överensstämmelsekrav"

#: src/main.sh:889
msgid "  --msg                 Print a msg message to stdout"
msgstr "  --msg Skriv ut ett meddelande till stdout"

#: src/main.sh:890
msgid "  --msg2                Print a msg2 message to stdout"
msgstr "  --msg2 Skriv ut ett msg2-meddelande till stdout"

#: src/main.sh:873
msgid ""
"  --no-build            Skip running of the 'build()' function in the "
"PKGBUILD"
msgstr "  --no-build Hoppa över körningen av 'build()'-funktionen i PKGBUILD"

#: src/main.sh:874
msgid ""
"  --no-check            Skip running of the 'check()' function in the "
"PKGBUILD"
msgstr "  --no-check Hoppa över körningen av 'check()'-funktionen i PKGBUILD"

#: src/main.sh:875
msgid "  --no-color            Disable colored output"
msgstr "  --no-color Inaktivera färgad utdata"

#: src/main.sh:883
msgid "  --no-confirm          Don't ask before installing packages"
msgstr "  --no-confirm Fråga inte innan du installerar paket"

#: src/main.sh:886
msgid "  --pass-env            Pass the current user's environment variables"
msgstr "  --pass-env Skicka med den aktuella användarens miljövariabler"

#: src/main.sh:876
msgid "  --print-control       Print a generated control file and exit"
msgstr "  --print-control Skriv ut en genererad kontrollfil och avsluta"

#: src/main.sh:877
msgid "  --print-srcinfo       Print a generated .SRCINFO file and exit"
msgstr "  --print-srcinfo Skriv ut en genererad .SRCINFO-fil och avsluta"

#: src/main.sh:878
msgid ""
"  --skip-pgp-check      Do not verify source files against PGP signatures"
msgstr "  --skip-pgp-check Verifiera inte källfiler mot PGP-signaturer"

#: src/main.sh:891
msgid "  --warning             Print a warning message to stderr"
msgstr "  --warning Skriv ut ett varningsmeddelande till stderr"

#: src/main.sh:892
msgid "  --warning2            Print a warning2 message to stderr"
msgstr "  --warning2 Skriv ut ett varning2 meddelande till stderr"

#: src/main.sh:862
msgid "  -A, --ignore-arch     Ignore errors about mismatching architectures"
msgstr "  -A, --ignore-arch Ignorera fel om felaktiga arkitekturer"

#: src/main.sh:864
msgid ""
"  -F, --file, -p        Specify a location to the build file (defaults to "
"'PKGBUILD')"
msgstr "  -F, --file, -p Ange en plats för byggfilen (standard är 'PKGBUILD')"

#: src/main.sh:867
msgid ""
"  -H, --field           Append the packaged control file with custom control "
"fields"
msgstr ""
"  -H, --field Lägg till den paketerade kontrollfilen med anpassade "
"kontrollfält"

#: src/main.sh:869
msgid "  -V, --version         Show version information and exit"
msgstr "  -V, --version Visa versionsinformation och avsluta"

#: src/main.sh:863
msgid "  -d, --no-deps         Skip all dependency checks"
msgstr "  -d, --no-deps Hoppa över alla beroendekontroller"

#: src/main.sh:865
msgid "  -g, --gen-integ       Generate hashes for source files"
msgstr "  -g, --gen-integ Generera hash för källfiler"

#: src/main.sh:866
msgid "  -h, --help            Show this help menu and exit"
msgstr "  -h, --help Visa denna hjälpmeny och avsluta"

#: src/main.sh:868
msgid ""
"  -i, --install         Automatically install the built package(s) after "
"building"
msgstr ""
"  -i, --install Installera automatiskt det/de byggda paketet/paketen efter "
"byggandet"

#: src/main.sh:870
msgid ""
"  -r, --rm-deps         Run 'apt-get autoremove' after a succesfull build"
msgstr "  -r, --rm-deps Kör 'apt-get autoremove' efter ett lyckat bygge"

#: src/main.sh:871
msgid "  -s, --sync-deps       Install missing dependencies"
msgstr "  -s, --sync-deps Installera saknade beroenden"

